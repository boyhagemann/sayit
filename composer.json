{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.1.*",
		"cviebrock/eloquent-sluggable": "1.0.*",
		"boyhagemann/form": "dev-master",
		"rtablada/package-installer": "dev-master",
		"lucadegasperi/oauth2-server-laravel": "1.x",
		"teepluss/api": "dev-master",
		"erusev/parsedown": "dev-master",
		"cartalyst/sentry": "2.1.*",
		"fadion/maneuver": "dev-master",
		"anahkiasen/rocketeer": "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/repositories",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
